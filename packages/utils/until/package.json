			"import": "./dist/index.mjs"
		},
		"./*": "./*"
	},
	"directories": {
		"dist": "dist",
		"src": "src"
	},
	"files": [
		"dist",
		"typings"
	],
	"dependencies": {
		"@uni-use/mounted": "workspace:*",
		"@uni-use/shared": "workspace:*",
		"js-cool": "^5.14.0",
		"tslib": "^2.6.2",
		"vue-demi": "^0.14.6"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.38.1",
		"vue": "^3.3.7",
		"vue2": "npm:vue@^2.7.15"
	},
	"peerDependencies": {
		"@vue/composition-api": ">=1.0.0",
		"vue": "^2.0.0 || >=3.0.0"
	},
	"peerDependenciesMeta": {
		"@vue/composition-api": {
			"optional": true
		}
	},
	"keywords": [
		"use-until",
		"until",
		"ssr",
		"useUntil",
		"saqqdy"
