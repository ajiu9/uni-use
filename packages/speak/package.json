{
	"name": "@uni-use/speak",
	"description": "A simple utility to get the package manager information which used in the project",
	"version": "1.0.0-beta.1",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"typings": "./dist/index.d.ts",
	"unpkg": "./index.iife.min.js",
	"jsdelivr": "./index.iife.min.js",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"require": "./dist/*.cjs",
			"import": "./dist/*.mjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {},
	"directories": {
		"es": "es",
		"lib": "lib",
		"src": "src",
		"typings": "typings"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.34.4",
		"eslint": "^8.34.0",
		"prettier": "^2.8.4"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"@babel/core",
				"webpack"
			],
			"allowedVersions": {}
		}
	},
	"keywords": [
		"uni-use",
		"use-speak"
	],
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"homepage": "https://github.com/saqqdy/uni-use/tree/master/packages/speak#readme",
	"bugs": {
		"url": "https://github.com/saqqdy/uni-use/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/uni-use.git",
		"directory": "packages/speak"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	}
}
